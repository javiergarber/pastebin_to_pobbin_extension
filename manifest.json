{
  "name": "Pastebin to Pobb.in",
  "version": "0.0.1",
  "description": "Loads the current pastebin at POBb.in",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["external-link.png"],
      "matches": ["https://pastebin.com/*"]
    }
  ],
  "icons": {
    "128": "128.png"
  },
  "content_scripts": [
    {
      "css": ["button.css"],
      "js": ["button.js"],
      "matches": ["https://pastebin.com/*"]
    }
  ]
}
